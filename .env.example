# n8n encryption key (generate with: openssl rand -hex 24)
N8N_ENCRYPTION_KEY=your_encryption_key_here

# PostgreSQL database password
POSTGRES_PASSWORD=your_secure_postgres_password

# AWS credentials for Bedrock
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1  # or your preferred region where Bedrock is available

# Slack API credentials
SLACK_OAUTH_TOKEN=xoxb-your-slack-oauth-token
SLACK_SIGNING_SECRET=your-slack-signing-secret

# pgAdmin credentials
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=your_secure_pgadmin_password